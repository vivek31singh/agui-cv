# OpenRouter API Configuration (Primary LLM Provider)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Model Configuration
# You can easily switch models by changing these variables
# Default: ZhipuAI GLM-4.6
RESUME_AGENT_MODEL=zhipuai-coding-plan/glm-4.6
# Optional: Specific API key if not using OpenRouter for this model
# ZHIPUAI_API_KEY=your_zhipuai_key_here

# CopilotKit Configuration (Optional)
# Get your keys from: https://cloud.copilotkit.ai
NEXT_PUBLIC_COPILOTKIT_PUBLIC_API_KEY=your_copilotkit_public_api_key_here
NEXT_PUBLIC_COPILOTKIT_PUBLIC_LICENSE_KEY=your_copilotkit_public_license_key_here

# Encryption Key for Client IP (Prevent warning)
# Generate a random 32-character string (e.g. using 'openssl rand -base64 32')
COPILOTKIT_CLIENT_IP_ENCRYPTION_KEY=your_random_generated_encryption_key_here

# Optional: Logging Configuration
# Options: debug, info, warn, error
# LOG_LEVEL=info
